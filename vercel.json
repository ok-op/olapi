{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12",
      "APT_INSTALL": "python3-distutils"
    }
  }
}
